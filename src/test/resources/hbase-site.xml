<configuration>
    <property>
        <name>hbase.client.connection.impl</name>
        <value>com.alicloud.tablestore.hbase.TablestoreConnection</value>
    </property>
    <property>
        <name>tablestore.client.endpoint</name>
        <value></value>
    </property>
    <property>
        <name>tablestore.client.instancename</name>
        <value></value>
    </property>
    <property>
